{
  "log": {
    "level": "error",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://mirror.ghproxy.com/https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "direct",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": false
    }
  },
  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      {
        "tag": "remote-dns",
        "address": "https://1.1.1.1/dns-query",
        "address_resolver": "remote-resolver-dns",
        "detour": "direct"
      },
      {
        "tag": "local-dns",
        "address": "https://8.8.8.8/dns-query",
        "address_resolver": "resolver-dns",
        "detour": "direct"
      },
      {
        "tag": "resolver-dns",
        "address": "1.1.1.1",
        "detour": "direct"
      },
      {
        "tag": "remote-resolver-dns",
        "address": "8.8.8.8",
        "detour": "direct"
      },
      {
        "tag": "fakeip-dns",
        "address": "fakeip"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "rule_set": "geosite-phishing",
        "server": "block"
      },
      {
        "rule_set": "geosite-malware",
        "server": "block"
      },
      {
        "rule_set": "geosite-cryptominers",
        "server": "block"
      },
      {
        "rule_set": "geosite-category-ads-all",
        "server": "block"
      },
      {
        "rule_set": "geosite-ir",
        "server": "local-dns"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "domain_suffix": [
              ".lan",
              ".localdomain",
              ".example",
              ".invalid",
              ".localhost",
              ".test",
              ".local",
              ".home.arpa"
            ],
            "invert": true
          },
          {
            "query_type": [
              "A",
              "AAAA"
            ]
          }
        ],
        "server": "fakeip-dns"
      },
      {
        "clash_mode": "direct",
        "server": "local-dns"
      },
      {
        "clash_mode": "global",
        "server": "remote-dns"
      }
    ],
    "final": "remote-dns",
    "strategy": "prefer_ipv4",
    "disable_cache": false,
    "disable_expire": false,
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun",
      "interface_name": "tun0",
      "domain_strategy": "prefer_ipv4",
      "inet4_address": "172.19.0.1/30",
      "inet6_address": "fdfe:dcba:9876::1/126",
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "stack": "system",
      "endpoint_independent_nat": false,
      "sniff": true,
      "sniff_override_destination": true,
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2080
        }
      }
    },
    {
      "type": "mixed",
      "tag": "mixed",
      "listen": "0.0.0.0",
      "listen_port": 2080,
      "sniff": true,
      "sniff_override_destination": true,
      "domain_strategy": "prefer_ipv4",
      "tcp_fast_open": true,
      "tcp_multi_path": true,
      "udp_fragment": true
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "type": "selector",
      "interrupt_exist_connections": true,
      "outbounds": [
        "auto",
        "{all}"
      ]
    },
    {
      "tag": "auto",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "300s",
      "tolerance": 50
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "override_android_vpn": true,
    "rules": [
      {
        "protocol": "dns",
        "port": 53,
        "outbound": "dns-out"
      },
      {
        "rule_set": "geoip-phishing",
        "outbound": "block"
      },
      {
        "rule_set": "geoip-malware",
        "outbound": "block"
      },
      {
        "rule_set": "geosite-phishing",
        "outbound": "block"
      },
      {
        "rule_set": "geosite-malware",
        "outbound": "block"
      },
      {
        "rule_set": "geosite-cryptominers",
        "outbound": "block"
      },
      {
        "rule_set": "geosite-category-ads-all",
        "outbound": "block"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": "geoip-ir",
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-ir",
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-ir",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-ir.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geoip-ir",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geoip-ir.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-category-ads-all",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-category-ads-all.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-cryptominers",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-cryptominers.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-malware",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-malware.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-phishing",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-phishing.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geoip-malware",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geoip-malware.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geoip-phishing",
        "type": "remote",
        "url": "https://cdn.jsdelivr.net/gh/chocolate4u/Iran-sing-box-rules@rule-set/geoip-phishing.srs"
      }
    ]
  }
}
