{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "TTAQI",
      "secret": "qolqoli",
      "access_control_allow_origin": [
        "http://127.0.0.1",
        "http://yacd.haishan.me"
      ],
      "access_control_allow_private_network": true,
	  "default_mode": "rule"
	}
  },
  "dns": {
    "servers": [
      {
        "tag": "remote-dns",
        "address": "https://chrome.cloudflare-dns.com/dns-query",
		"address_resolver": "1.0.0.1",
		"strategy": "prefer_ipv4",
        "detour": "TTAQI"
      },
      {
        "tag": "direct-dns",
        "address": "https://dns.google/dns-query",
        "address_resolver": "8.8.4.4",
        "strategy": "prefer_ipv4",
        "detour": "TTAQI"
      },
      {
        "tag": "local-dns",
        "address": "local",
        "strategy": "ipv4_only",
        "detour": "TTAQI"
      },
      {
        "tag": "block-dns",
        "address": "rcode://refused"
      },
      {
        "tag": "fakeip-dns",
        "address": "fakeip"
      }
    ],
    "rules": [
      {
        "rule_set": [
          "geosite-phishing",
          "geosite-malware",
          "geosite-cryptominers",
          "geosite-category-ads-all"
        ],
        "server": "block-dns",
		"disable_cache": true,
		"rewrite_ttl": 0
      },
      {
        "rule_set": [
        "geosite-private"
        ],
        "server": "local-dns"
      },
      {
        "domain_suffix": [
          ".ir"
        ],
        "server": "direct-dns",
        "disable_cache": false,
        "rewrite_ttl": 20
      },
      {
        "rule_set": [
        "geosite-ir"
        ],
        "server": "direct-dns",
        "disable_cache": false,
        "rewrite_ttl": 20
      },
      {
        "clash_mode": "direct",
        "server": "direct-dns"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-ir",
            "invert": true
          },
          {
            "query_type": [
              "A",
              "AAAA"
            ]
          }
        ],
        "server": "fakeip-dns",
        "rewrite_ttl": 1
      },
      {
        "clash_mode": "global",
        "server": "remote-dns"
      }
    ],
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "final": "remote-dns",
    "strategy": "ipv4_only",
    "disable_cache": false,
    "disable_expire": false,
    "reverse_mapping": true,
    "independent_cache": true
  },
  "inbounds": [
    {
	  "tag": "tun-in",
	  "type": "tun",
      "interface_name": "tun0",
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
	  "sniff": true,
	  "sniff_override_destination": false,
      "stack": "system",
      "address": [
        "172.19.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 7890
        }
      }
    },
	{
      "tag": "mixed-in",
	  "type": "mixed",
	  "listen": "0.0.0.0",
	  "listen_port": 2080,
      "sniff": true
    }
  ],
  "outbounds": [
    {
      "tag":"QQOLI",
      "type":"selector",
	  "interrupt_exist_connections": false,
      "outbounds":[
        "MMOBI",
        "{all}"
      ]
    },
   {
      "tag":"MMOBI",
      "type":"urltest",
      "url": "https://www.gstatic.com/generate_204",
	  "interrupt_exist_connections": false,
      "interval": "5m",
      "tolerance": 50,
      "outbounds":[
        "{all}"
      ]
    },
    {
      "tag": "TTAQI",
      "type": "direct"
    },
    {
      "tag": "NNAQI",
      "type": "dns"
    },
    {
      "tag": "MMAQI",
      "type": "block"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "override_android_vpn": true,
    "final": "QQOLI",
    "rules": [
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "protocol": "dns"
          },
          {
            "port": 53
          }
        ],
        "outbound": "NNAQI"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "network": "udp"
          },
          {
            "port": 443
          }
        ],
        "outbound": "MMAQI"
      },
      {
        "rule_set": [
          "geosite-category-ads-all",
