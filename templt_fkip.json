{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    }
  },
  "dns": {
    "servers": [
      {"tag":"remote-dns","address":"https://1.1.1.1/dns-query","strategy":"prefer_ipv4"},
      {"tag":"backup-dns","address":"https://9.9.9.9/dns-query","strategy":"prefer_ipv4"},
      {"tag":"direct-dns","address":"8.8.8.8","strategy":"prefer_ipv4"},
      {"tag":"block-dns","address":"rcode://refused"}
    ],
    "rules": [
      {"outbound":"MMAQI","server":"block-dns","disable_cache":true},
      {"outbound":"TTAQI","server":"direct-dns","disable_cache":false},
      {"outbound":"QQOLI","server":"remote-dns","disable_cache":false},
      {"outbound":"any","server":"direct-dns","disable_cache":false},
      {"domain_suffix":[".githubusercontent.com",".githubassets.com",".github.com"],"server":"direct-dns","disable_cache":false},
      {"query_type":["A","AAAA"],"server":"fakeip-dns"},      
      {"clash_mode":"Direct","server":"direct-dns","disable_cache":false},
      {"clash_mode":"Global","server":"remote-dns","disable_cache":false},
      {"domain_suffix":[".xiaomi.com",".xiaomi.net",".miui.com",".mi.com"],"server":"block-dns","disable_cache":true,"rewrite_ttl":0},
      {"domain_suffix":".ir","server":"direct-dns","disable_cache":false}
    ],
    "fakeip":{"enabled":true,"inet4_range":"198.18.0.0/15","inet6_range":"fc00::/18"},
    "independent_cache": true,
    "strategy": "prefer_ipv4",
    "final": "backup-dns"
  },
  "ntp": {
    "enabled": true,
    "server": "pool.ntp.org",
    "server_port": 123,
    "interval": "30m",
    "detour": "TTAQI"
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "mtu": 9000,
      "auto_route": true,
      "strict_route": false,
      "endpoint_independent_nat": true,
      "sniff": true,
      "sniff_override_destination": true,
      "stack": "mixed",
      "address": [
        "172.19.0.1/30"
      ],
      "platform": {"http_proxy":{"enabled":true,"server":"127.0.0.1","server_port":7890}},
      "route_exclude_address":["162.159.192.0/24","162.159.193.0/24","162.159.195.0/24","188.114.96.0/24","188.114.97.0/24","188.114.98.0/24","188.114.99.0/24","2606:4700:d0::/64","2606:4700:d1::/64"]      
    },
    {
      "tag": "mixed-in",
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 7891,
      "domain_strategy": "prefer_ipv4",
      "sniff": true,
      "sniff_override_destination": true
    }
  ],
  "outbounds": [
    {
      "tag":"QQOLI",
      "type":"selector",
      "interrupt_exist_connections": false,
      "outbounds":["MMOBI","{all}"]
    },
   {
      "tag":"MMOBI",
      "type":"urltest",
      "url": "https://www.gstatic.com/generate_204",
      "interrupt_exist_connections": false,
      "interval": "5m",
      "tolerance": 50,
      "outbounds":["{all}"]
    },
    {
      "tag": "TTAQI",
      "type": "direct"
    },
    {
      "tag": "NNAQI",
      "type": "dns"
    },
    {
      "tag": "MMAQI",
      "type": "block"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "override_android_vpn": true,
    "final": "QQOLI",
    "rules": [
      {"protocol":"dns","outbound":"NNAQI"},
      {"domain_suffix":[".githubusercontent.com",".githubassets.com",".github.com"],"outbound":"TTAQI"},
      {"type":"logical","mode":"and","rules":[{"network":"udp"},{"port":443}],"outbound":"MMAQI"},
      {"protocol":"quic","outbound":"MMAQI"},
      {"domain_suffix":[".xiaomi.com",".xiaomi.net",".miui.com",".mi.com"],"outbound":"MMAQI"},
      {"ip_cidr":["0.0.0.0/32","10.10.34.0/24","240.0.0.0/4"],"outbound":"MMAQI"},
      {"ip_is_private":true,"outbound":"TTAQI"},
      {"domain_suffix":".ir","outbound":"TTAQI"},
      {"clash_mode":"Direct","outbound":"TTAQI"},
      {"clash_mode":"Global","outbound":"QQOLI"}
    ],
    "rule_set": []
  }
}
