          "geoip-phishing",
          "geosite-phishing",
          "geoip-malware",
          "geosite-malware"
        ],
        "outbound": "MMAQI"
      },
      {
        "ip_cidr": [
          "0.0.0.0/32",
          "10.10.34.0/24",
          "240.0.0.0/4"
        ],
        "outbound": "MMAQI"
      },
      {
        "rule_set": [
          "geosite-private"
        ],
        "outbound": "TTAQI"
      },
      {
        "ip_is_private": true,
        "outbound": "TTAQI"
      },
      {
        "domain_suffix": [
          ".ir"
        ],
        "outbound": "TTAQI"
      },
      {
        "rule_set": [
          "geosite-ir"
        ],
        "outbound": "TTAQI"
      },
      {
        "rule_set": [
          "geoip-ir"
        ],
        "outbound": "TTAQI"
