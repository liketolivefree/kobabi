{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "TTAQI",
      "secret": "qolqoli",
      "access_control_allow_origin": [
        "http://127.0.0.1",
        "http://yacd.haishan.me"
      ],
      "access_control_allow_private_network": true,
	  "default_mode": "rule"
	}
  },
  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      {
        "tag": "remote-dns",
        "address": "https://cloudflare-dns.com/dns-query",
        "address_resolver": "resolver-dns",
		"address_strategy": "prefer_ipv4",
        "strategy": "ipv4_only",
        "detour": "QQOLI"
      },
      {
        "tag": "direct-dns",
        "address": "https://dns.shecan.ir/dns-query",
        "address_resolver": "resolver-dns",
		"address_strategy": "prefer_ipv4",
        "strategy": "ipv4_only",
        "detour": "TTAQI"
      },
      {
        "tag": "resolver-dns",
        "address": "tcp://1.0.0.1",
        "strategy": "ipv4_only",
        "detour": "TTAQI"
      },
      {
        "tag": "block-dns",
        "address": "rcode://refused"
      },
      {
        "tag": "fakeip-dns",
        "address": "fakeip"
      }
    ],
    "rules": [
      {
        "rule_set": [
		  "geosite-phishing",
		  "geosite-malware",
		  "geosite-cryptominers",
		  "geosite-category-ads-all"
		],
        "server": "block-dns"
      },
      {
        "outbound": "any",
        "server": "resolver-dns"
      },
      {
        "domain_suffix": [
          ".ir"
        ],
        "server": "direct-dns"
      },
      {
        "rule_set": "geosite-ir",
        "server": "direct-dns"
      },
      {
        "clash_mode": "direct",
        "server": "direct-dns"
      },
      {
        "clash_mode": "global",
        "server": "remote-dns"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-ir",
            "invert": true
          },
          {
            "query_type": [
              "A",
              "AAAA"
            ]
          }
        ],
        "server": "dns-fakeip"
      }
    ],
    "final": "remote-dns",
    "strategy": "ipv4_only",
    "disable_cache": false,
    "disable_expire": false,
    "reverse_mapping": false,
    "independent_cache": true
  },
  "ntp": {
    "enabled": false,
    "server": "pool.ntp.org",
    "server_port": 123,
    "interval": "30m",
    "detour": "TTAQI"
  },
  "inbounds": [
    {
	  "tag": "tun-in",
	  "type": "tun",
      "interface_name": "tun0",
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "stack": "system",
      "domain_strategy": "ipv4_only",
      "endpoint_independent_nat": false,
      "address": [
        "172.19.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "route_address": [
        "0.0.0.0/1",
        "128.0.0.0/1",
        "::/1",
        "8000::/1"
      ],
      "route_exclude_address": [
        "192.168.0.0/16",
        "fc00::/7"
      ],
      "exclude_package": [
        "com.android.captiveportallogin"
      ],
      "platform": {
        "http_proxy": {
          "enabled": false,
          "server": "127.0.0.1",
          "server_port": 7890
        }
      }
    }
  ],
  "outbounds": [
    {
      "tag":"QQOLI",
      "type":"selector",
	  "interrupt_exist_connections": false,
      "outbounds":[
        "MMOBI",
        "{all}"
      ]
    },
   {
      "tag":"MMOBI",
      "type":"urltest",
      "url": "https://www.gstatic.com/generate_204",
	  "interrupt_exist_connections": false,
      "interval": "5m",
      "tolerance": 15,
      "outbounds":[
        "{all}"
      ]
    },
    {
      "tag": "TTAQI",
      "type": "direct"
    },
    {
      "tag": "NNAQI",
      "type": "dns"
    },
    {
      "tag": "MMAQI",
      "type": "block"
    }
  ],
